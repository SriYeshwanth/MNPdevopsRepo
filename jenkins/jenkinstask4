pipeline {
    agent any

    stages {
        stage('Validation') {
            steps {
                script {
                     sh '''
echo "hello ${USER}"

if '[ ${USER} == ^[a-z]*[A-Z]*[0-9]$ ]'
then
echo " good to enter ${USER} "
else
echo "not good to enter ${USER}"
fi
'''
                }
                }
            }
        }
    }
